{
  "cleaners": [
    "remove_unnamed",
    "get_release_year",
    "running_time_mins"
  ],
  "fillers": [
    "release_date"
  ],
  "tasks": {
    "movie_desc_embeddings_runner": {
      "embeddings_path": "data/embeddings.npy",
      "n_components": 2,
      "tsne": {
        "init": "random",
        "tsne_x_path": "data/tsne_x.npy",
        "tsne_y_path": "data/tsne_y.npy"
      },
      "pca": {
        "pca_x_path": "data/pca_x.npy",
        "pca_y_path": "data/pca_y.npy"
      }
    },
    "content_based_filtering_runner": {
      "tfidf": {
        "similarity_matrix_path": "data/similarity_matrix.npy",
        "df_mapping_path": "data/df_mapping"
      }
    }
  },
  "run_tasks": {
    "movie_desc_embeddings_runner": [
      "clean_lemmatize",
      "get_embeddings",
      "get_pca_coords",
      "get_tsne_coords"
    ],
    "content_based_filtering_runner": [
      "compute_similarity_matrix"
    ]
  }
}